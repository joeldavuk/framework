<?xml version="1.0" encoding="UTF-8" ?>
<locale>
    <name>bg-BG</name>
    <displayName>Bulgarian (Bulgaria)</displayName>
    <datetime>
        <nativeCalendar>Грегориански календар</nativeCalendar>
        <shortDatePattern>d.M.yyyy 'г.'</shortDatePattern>
        <longDatePattern>dd MMMM yyyy 'г.'</longDatePattern>
        <dateSeparator>.</dateSeparator>
        <timeSeparator>:</timeSeparator>
        <dayNames>
            <fullName>
                <name>неделя</name>
                <name>понеделник</name>
                <name>вторник</name>
                <name>сряда</name>
                <name>четвъртък</name>
                <name>петък</name>
                <name>събота</name>

            </fullName>
            <shortName>
                <name>нед</name>
                <name>пон</name>
                <name>вт</name>
                <name>ср</name>
                <name>четв</name>
                <name>пет</name>
                <name>съб</name>

            </shortName>
        </dayNames>
        <monthNames>
            <fullName>
                <name>януари</name>
                <name>февруари</name>
                <name>март</name>
                <name>април</name>
                <name>май</name>
                <name>юни</name>
                <name>юли</name>
                <name>август</name>
                <name>септември</name>
                <name>октомври</name>
                <name>ноември</name>
                <name>декември</name>

            </fullName>
            <shortName>
                <name>ян</name>
                <name>февр</name>
                <name>март</name>
                <name>апр</name>
                <name>май</name>
                <name>юни</name>
                <name>юли</name>
                <name>авг</name>
                <name>септ</name>
                <name>окт</name>
                <name>ноември</name>
                <name>дек</name>

            </shortName>
        </monthNames>
    </datetime>
    <numberFormat>
        <currencySymbol>лв.</currencySymbol>
        <numberDecimalDigits>2</numberDecimalDigits>
        <currencyGroupSeparator> </currencyGroupSeparator>
        <currencyDecimalSeparator>,</currencyDecimalSeparator>
        <currencyPositivePattern>3</currencyPositivePattern>
    </numberFormat>
</locale>
